<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Projet\FreelancerBundle\Entity\JobOwner" table="job_owner">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="email" type="string" column="email" length="50" nullable="false"/>
    <field name="motdepasse" type="string" column="motdepasse" length="20" nullable="false"/>
    <field name="nom" type="string" column="nom" length="50" nullable="false"/>
    <field name="prenom" type="string" column="prenom" length="50" nullable="false"/>
    <field name="pays" type="string" column="pays" length="20" nullable="false"/>
    <field name="ville" type="string" column="ville" length="20" nullable="false"/>
    <field name="addresse" type="text" column="addresse" nullable="false"/>
    <field name="telephone" type="string" column="telephone" length="20" nullable="false"/>
    <field name="entreprise" type="string" column="entreprise" length="50" nullable="false"/>
    <field name="jeton" type="integer" column="jeton" nullable="false"/>
  </entity>
</doctrine-mapping>
