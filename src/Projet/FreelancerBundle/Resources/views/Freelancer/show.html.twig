{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Freelancer</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Email</th>
                
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Motdepasse</th>
                <td>{{ entity.motdepasse }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ entity.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ entity.prenom }}</td>
            </tr>
            <tr>
                <th>Pays</th>
                <td>{{ entity.pays }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ entity.ville }}</td>
            </tr>
            <tr>
                <th>Addresse</th>
                <td>{{ entity.addresse }}</td>
            </tr>
            <tr>
                <th>Cv</th>
                <td>{{ entity.cv }}</td>
            </tr>
            <tr>
                <th>Specialite</th>
                <td>{{ entity.specialite }}</td>
            </tr>
            <tr>
                <th>Jeton</th>
                <td>{{ entity.jeton }}</td>
            </tr>
            <tr>
                <th>Evaluation</th>
                <td>{{ entity.evaluation }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ entity.telephone }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('freelancer') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('freelancer_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
